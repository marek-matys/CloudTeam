#!/bin/bash

echo "Run date $(date)"

cd /home/ec2-user/

aws s3 mv s3://kjy01rm8fv/in/ ./in/ --include "*.csv" --recursive 

if [ $? -ne 0 ];then exit; fi

for i in $(ls ./in/)
do
	echo "Processing $i"
	./processing.sh ./in/$i > ./out/processed-$i
	if [ $? -ne 0 ];then exit;fi
	rm ./in/$i
done

if [ $? -ne 0 ];then exit;fi

aws s3 mv ./out/ s3://kjy01rm8fv/out/ --include "*.csv" --recursive



